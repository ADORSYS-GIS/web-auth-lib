{
  "mcpServers": {
    "git": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "-v", "local:/workspace", "mcp/git"],
      "disabled": true,
      "alwaysAllow": []
    },
    "filesystem": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "-v", "local:/workspace", "mcp/filesystem", "/workspace"],
      "disabled": true,
      "alwaysAllow": []
    }
  }
}
